#jinja2: trim_blocks: True, lstrip_blocks: True
# {{ ansible_managed }}
{% if mysql_conf_client_vars is defined and mysql_conf_client_vars|length > 0 %}
{% for block in mysql_conf_client_vars %}
{% if block['name'] is defined %}
[{{block['name']}}]
{% endif %}
{% for k,v in block.items() %}
{% if k == 'vars' %}
{% for confitem in v %}
{% for key,val in confitem.items() %}
{{key}} = {{val}}
{% endfor %}
{% endfor %}
{% endif %}
{% if k == 'vals' %}
{% for myval in v %}
{{myval}}
{% endfor %}
{% endif %}
{% endfor %}

{% endfor %}
{% endif %}